%li
  = link_to intervention, class: 'card' do
    .main
      .details
        %h3= intervention.title
        %p= intervention.summary
        .tags= intervention.tag_list
    - intervention.outcomes.each do |o|
      .measure-wrap
        .outcome
          .label
            %strong= o.title
        .effect
          = o.effect_meter
        .evidence
          = o.evidence_meter
